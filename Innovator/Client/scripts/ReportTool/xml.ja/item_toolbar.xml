<?xml version="1.0" encoding="UTF-8"?>
<toolbarapplet
	on_click = "onClickItem"
	on_load  = "onToolbarLoad"
	buttonstyle = "windows"
	buttonsize = "22,22"
	font = "Arial-12"
	top_border = "1"
	bottom_border = "1">
	<toolbar id="normal">
		<button image="../images/Print.svg" id="print" tooltip="アイテムを印刷" disabled="true">印刷 </button>
		<separator/>
		<button image="../images/Delete.svg" id="delete" tooltip="アイテムを削除" disabled="true">削除 </button>
		<separator/>
		<button image="../images/LockItem.svg" id="lock" tooltip="アイテムをロック" disabled="true" >ロック </button>
		<button image="../images/UnockItem.svg" id="unlock" tooltip="アイテムをアンロック" disabled="true">アンロック </button>
		<button image="../images/Rollback.svg" id="undo" tooltip="アイテムの変更を元に戻す" disabled="true">元に戻す </button>
		<separator/>
		<button image="../images/Save.svg" id="save" tooltip="保存" disabled="true" >Save </button>
		<button image="../images/SaveUnlockClose.svg" id="save_unlock_close" tooltip="保存、アンロックと閉じる" disabled="true">保存と閉じる </button>
	</toolbar>
</toolbarapplet>