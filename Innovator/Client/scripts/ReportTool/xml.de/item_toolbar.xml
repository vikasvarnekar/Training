<?xml version="1.0" encoding="UTF-8"?>
<toolbarapplet
	on_click = "onClickItem"
	on_load  = "onToolbarLoad"
	buttonstyle = "windows"
	buttonsize = "22,22"
	font = "Arial-12"
	top_border = "1"
	bottom_border = "1">
	<toolbar id="normal">
		<button image="../images/Print.svg" id="print" tooltip="Objekt drucken" disabled="true">Drucken </button>
		<separator/>
		<button image="../images/Delete.svg" id="delete" tooltip="Objekt löschen" disabled="true">Löschen </button>
		<separator/>
		<button image="../images/LockItem.svg" id="lock" tooltip="Objekt sperren" disabled="true" >Sperren </button>
		<button image="../images/UnockItem.svg" id="unlock" tooltip="Objekt entsperren" disabled="true">Entsperren </button>
		<button image="../images/Rollback.svg" id="undo" tooltip="Änderungen zurücksetzen" disabled="true">Änderungen zurücksetzen </button>
		<separator/>
		<button image="../images/Save.svg" id="save" tooltip="Speichern" disabled="true" >Speichern </button>
		<button image="../images/SaveUnlockClose.svg" id="save_unlock_close" tooltip="Speichern, Entsperren &amp; Schließen" disabled="true">Speichern und Schließen </button>
	</toolbar>
</toolbarapplet>