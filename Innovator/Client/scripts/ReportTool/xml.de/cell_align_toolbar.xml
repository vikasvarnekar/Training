<toolbarapplet
  on_click        = "onClickItem"
  on_load         = "onToolbarLoad"
  buttonstyle     = "windows"
  buttonsize      = "22,22"
  font            = "Arial-12"
  bottom_border   = "1">

  <toolbar id="normal">
    <button image="../scripts/ReportTool/images/top-left.jpg"      id="top-left"      tooltip="Top Left"     >Oben links  </button>
    <button image="../scripts/ReportTool/images/top-center.jpg"    id="top-center"    tooltip="Top Center"   >Oben mittig  </button>
    <button image="../scripts/ReportTool/images/top-right.jpg"     id="top-right"     tooltip="Top Right"    >Oben rechts  </button>
    <button image="../scripts/ReportTool/images/middle-left.jpg"   id="middle-left"   tooltip="Middle Left"  >Mitte links  </button>
    <button image="../scripts/ReportTool/images/middle-center.jpg" id="middle-center" tooltip="Middle Center">Mitte mittig  </button>
    <button image="../scripts/ReportTool/images/middle-right.jpg"  id="middle-right"  tooltip="Middle Right" >Mitte rechts  </button>
    <button image="../scripts/ReportTool/images/bottom-left.jpg"   id="bottom-left"   tooltip="Bottom Left"  >Unten links  </button>
    <button image="../scripts/ReportTool/images/bottom-center.jpg" id="bottom-center" tooltip="Bottom Center">Unten mittig  </button>
    <button image="../scripts/ReportTool/images/bottom-right.jpg"  id="bottom-right"  tooltip="Bottom Right" >Unten rechts  </button>
  </toolbar>

</toolbarapplet>

