{
  "id" : "home_page_sidebar_search_box",
  "type"  : "wrapperGroup",
  "extraclass": "skin-component-properties",
  "wrapper": "divElement",
  "items" : [
    {
      "type": "collapsible",
      "label": "General",
      "expanded": "@uiStates.home_page_sidebar_search_box_general",
      "defaultState": true,
      "items": [
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Search Options Color"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.search_box_mobile_options_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "search_box_mobile_options_color"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Search Options Checkbox Image"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "search_box_mobile_options_checkbox_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.search_box_mobile_options_checkbox_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Search Options Checkbox Selected Image"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "search_box_mobile_options_checkbox_selected_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.search_box_mobile_options_checkbox_selected_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Search Image"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "sidebar_search_box_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.sidebar_search_box_image"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "collapsible",
      "label": "Autocomplete",
      "expanded": "@uiStates.home_page_sidebar_search_box_autocomplete",
      "defaultState": true,
      "items": [
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Item Text Color"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.sidebar_autocomplete_item_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "sidebar_autocomplete_item_color"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Item Background Color"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.sidebar_autocomplete_item_bgcolor",
              "on-change": {"name": "colorChanged", "args": {"attr": "sidebar_autocomplete_item_bgcolor"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Item Hover Color"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.sidebar_autocomplete_item_hover_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "sidebar_autocomplete_item_hover_color"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Separator Color"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.sidebar_autocomplete_seperator_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "sidebar_autocomplete_seperator_color"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Remove Button Image"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "sidebar_autocomplete_remove_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.sidebar_autocomplete_remove_image"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "collapsible",
      "label": "Search Field",
      "expanded": "@uiStates.home_page_sidebar_search_box_field",
      "defaultState": true,
      "items": [
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Text"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.search_box_mobile_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "search_box_mobile_color"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Field Background"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.search_box_mobile_field_bgcolor",
              "on-change": {"name": "colorChanged", "args": {"attr": "search_box_mobile_field_bgcolor"}}
            }
          ]
        }
      ]
    }
  ]
}
