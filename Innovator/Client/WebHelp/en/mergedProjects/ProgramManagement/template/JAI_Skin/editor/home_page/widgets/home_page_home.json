{
  "id" : "home_page_home",
  "type"  : "wrapperGroup",
  "extraclass": "skin-component-properties",
  "wrapper": "divElement",
  "items" : [
    {
      "type": "collapsible",
      "label": "Colors",
      "expanded": "@uiStates.home_page_home_color",
      "defaultState": true,
      "items": [
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Title"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.home_page_title_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "home_page_title_color"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Subtitle"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.home_page_subtitle_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "home_page_subtitle_color"}}
            }
          ]
        }
      ]
    }
  ]
}
