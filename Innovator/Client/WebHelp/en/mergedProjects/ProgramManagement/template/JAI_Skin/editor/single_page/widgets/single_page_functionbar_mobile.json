{
  "id" : "single_page_functionbar_mobile",
  "type"  : "wrapperGroup",
  "extraclass": "skin-component-properties",
  "wrapper": "divElement",
  "items" : [
    {
      "type": "collapsible",
      "label": "Colors",
      "expanded": "@uiStates.single_page_functionbar_mobile_color",
      "defaultState": true,
      "items": [
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Background"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.functionbar_mobile_bgcolor",
              "on-change": {"name": "colorChanged", "args": {"attr": "functionbar_mobile_bgcolor"}}
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Border"
            },
            {
              "type": "colorpicker",
              "extraclass": "coral-Form-field",
              "data": "attrs.functionbar_mobile_border_color",
              "on-change": {"name": "colorChanged", "args": {"attr": "functionbar_mobile_border_color"}}
            }
          ]
        }
      ]
    },
    {
      "type": "collapsible",
      "label": "Images",
      "expanded": "@uiStates.single_page_functionbar_mobile_images",
      "defaultState": true,
      "items": [
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Previous Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "previous_button_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.previous_button_mobile_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Previous Active Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "previous_button_active_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.previous_button_active_mobile_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Next Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "next_button_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.next_button_mobile_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Next Active Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "next_button_active_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.next_button_active_mobile_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Filter Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "filter_button_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.filter_button_mobile_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Filter Applied Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "filter_applied_button_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.filter_applied_button_mobile_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Search Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "search_button_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.search_button_mobile_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Search Active Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "search_button_mobile_active_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.search_button_mobile_active_image"
              }
            }
          ]
        },
        {
          "type": "divElement2",
          "extraclass": "group",
          "items": [
            {
              "type": "label",
              "extraclass": "coral-FieldLabel",
              "label": "Menu Button"
            },
            {
              "type": "widget",
              "id"  : "media_source",
              "changeEvent": "imageChanged",
              "hrefAttrName": "menu_button_mobile_image",
              "inputModel": {
                "variant": "secondary",
                "basePath": "@basePath",
                "filePath": "@attrs.menu_button_mobile_image"
              }
            }
          ]
        }
      ]
    }
  ]
}
