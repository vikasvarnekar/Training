<?xml version="1.0" encoding="UTF-8"?>
<dialogs>
	<find title="Search">
		<row>
			<textbox name="textBoxFind" style="DialogWidget TextBox" width="172"></textbox>
			<button name="buttonNextMatch" src="../../../images/ArrowRight.svg" style="DialogWidget SmallButton"></button>
		</row>
		<row>
			<checkbox name="checkboxMatchCase" label="Match Case" style="DialogWidget"></checkbox>
		</row>
	</find>
	<scribblePalette title="Scribble">
		<row>
			<button name="btnScribble" src="../../../images/Scribble.svg" tooltip="Griffonnage" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="Sélectionner" style="SelectButton"></button>
		</row>
		<row>
			<button name="smallScribbleButton" src="../../../images/ScribbleSmall.svg" tooltip="Petit" style="DrawingSizeButton"></button>
			<button name="mediumScribbleButton" src="../../../images/ScribbleMedium.svg" tooltip="Moyen" style="DrawingSizeButton"></button>
			<button name="largeScribbleButton" src="../../../images/ScribbleLarge.svg" tooltip="Grand" style="DrawingSizeButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Yellow"></color>
					<color color="#008400" title="Green"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="White"></color>
				</row>
				<row>
					<color color="#fd0200" title="Red" selected="selected"></color>
					<color color="#002a99" title="Blue"></color>
					<color color="#808080" title="Grey"></color>
					<color color="#000" title="Black"></color>
				</row>
			</colorpallete>
		</row>
	</scribblePalette>
	<highlightPalette title="Highlight">
		<row>
			<button name="btnHighlight" src="../../../images/Highlight.svg" tooltip="Mettre en relief" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="Sélectionner" style="SelectButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Yellow" selected="selected"></color>
					<color color="#4bff4b" title="Green"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="White"></color>
				</row>
				<row>
					<color color="#ff37b6" title="Pink"></color>
					<color color="#01aff1" title="Blue"></color>
					<color color="#808080" title="Grey"></color>
					<color color="#000" title="Black"></color>
				</row>
			</colorpallete>
		</row>
	</highlightPalette>
	<labelPalette title="Label">
		<row>
			<button name="btnLabel" src="../../../images/Label.svg" tooltip="Label" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="Sélectionner" style="SelectButton"></button>
		</row>
		<row>
			<button name="smallLabelButton" src="../../../images/LabelSmall.svg" tooltip="Petit" style="DrawingSizeButton"></button>
			<button name="mediumLabelButton" src="../../../images/LabelMedium.svg" tooltip="Moyen" style="DrawingSizeButton"></button>
			<button name="largeLabelButton" src="../../../images/LabelLarge.svg" tooltip="Grand" style="DrawingSizeButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Yellow"></color>
					<color color="#008400" title="Green"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="White"></color>
				</row>
				<row>
					<color color="#fd0200" title="Red" selected="selected"></color>
					<color color="#002a99" title="Blue"></color>
					<color color="#808080" title="Grey"></color>
					<color color="#000" title="Black"></color>
				</row>
			</colorpallete>
		</row>
	</labelPalette>
	<measure title="Measure" style="MeasureDialogContainer">
		<row style="MeasureDialogContentRow">
			<button name="btnMeasurePointToPoint" src="../../../images/MeasurePointToPoint.svg" tooltip="Mesurer du point au point" style="ViewerButtonCell MeasureFirstButton"></button>
			<button name="btnMeasureAngleBetweenFaces" src="../../../images/MeasureAngleBetweenFaces.svg" tooltip="Mesurer l'angle entre les surfaces" style="ViewerButtonCell MeasureSecondButton"></button>
			<button name="btnDeleteAll" src="../../../images/Delete.svg" tooltip="Supprimer tout" style="ViewerButtonCell MeasureThirdButton"></button>
		</row>
		<row style="MeasureDialogContentRow">
			<button name="btnMeasureEdges" src="../../../images/MeasureEdges.svg" tooltip="Mesurer les extrémités" style="ViewerButtonCell MeasureFourthButton"></button>
			<button name="btnMeasureDistanceBetweenFaces" src="../../../images/MeasureDistanceBetweenFaces.svg" tooltip="Mesurer la distance entre les surfaces" style="ViewerButtonCell MeasureFifthButton"></button>
			<empty style="ViewerButtonCell ViewerButtonCellEmpty"></empty>
		</row>
	</measure>
	<crossSection title="Cross Section">
			<row style="CrossSectionContentRow">
				<button name="xButton" src="../../../images/XPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Plan X"></button>
				<button name="yButton" src="../../../images/YPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Plan Y"></button>
				<button name="zButton" src="../../../images/ZPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Plan Z"></button>
				<button name="pButton" src="../../../images/PickPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Choisir le plan"></button>

				<button name="tcpvButton" src="../../../images/CuttingPlaneVisibilityToggle.svg" style="DialogWidget PaddedLeftButton SmallButton" tooltip="Basculer l'affichage du plan coupé"></button>
			</row>
			<row>
				<button name="ixButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Inverser la section"></button>
				<button name="iyButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Inverser la section"></button>
				<button name="izButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Inverser la section"></button>
				<button name="ipButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Inverser la section"></button>

				<button name="tcpsButton" src="../../../images/CuttingPlaneSectionToggle.svg" style="DialogWidget PaddedLeftButton SmallButton" tooltip="Basculer la section transversale"></button>
			</row>
		<row style="CappingGeometryRow">
			<checkbox name="checkboxСappingGeometry" label="Capped Geometry" style="DialogWidget"></checkbox>
		</row>

	</crossSection>
	<exploded title="Exploded View">
		<row>
			<slider name="slider" style="DialogWidgetSlider"></slider>
		</row>
	</exploded>
	<comparePallete title="Compare">
		<row style="CompareDialogContentRow">
			<button name="btnRollback" src="../../../images/Rollback.svg" tooltip="Réinitialiser l'alignement" style="CompareButton"></button>
			<button name="btnAlignPointToPoint" src="../../../images/AlignPointToPoint.svg" tooltip="Glisser pour aligner
Utiliser les flèches pour l'aligmenent juste" style="CompareButton"></button>
			<button name="btnRefresh" src="../../../images/Refresh.svg" tooltip="Rafraîchir" style="CompareButton"></button>
		</row>
		<row style="CompareDialogContentRow">
			<button name="btnDisplayFile1" src="../../../images/File1Indicator.svg" tooltip="Afficher Fichier 1" style="CompareButton"></button>
			<button name="btnDisplayFile2" src="../../../images/File2Indicator.svg" tooltip="Afficher Fichier 2" style="CompareButton"></button>
			<button name="btnDisplayBothFiles" src="../../../images/DisplayBothFiles.svg" tooltip="Afficher les deux fichiers" style="CompareButton"></button>
		</row>
		<row style="CompareDialogContentRow">
			<button name="btnDifferenceView" src="../../../images/ShowComparison.svg" tooltip="Afficher la différence" style="CompareButton" type="parent">Difference</button>
		</row>
		<row style="CompareDialogContentRow CompareToggle">
			<slider name="sliderTolerance" style="DialogWidgetSlider CompareButton"></slider>
		</row>
		<row style="CompareDialogContentRow CompareToggle">
			<colorpallete name="palleteDifferenceColor" style="CompareButton">
				<row>
					<color color="#fdff00" title="Yellow" selected="selected"></color>
					<color color="#4bff4b" title="Green"></color>
					<color color="#fc0001" title="Red"></color>
					<color color="#02249c" title="Blue"></color>
					<color color="#ff37b6" title="Pink"></color>
					<color color="#01aff1" title="Light Blue"></color>
				</row>
			</colorpallete>
		</row>
	</comparePallete>
	<preferences title="Preferences">
		<row>
			<label>Resolution:</label>
		</row>
		<row style="PreferencesRadioButtonContainer">
			<radiobutton name="defaultResolution" groupName="preferences" text="Use system default"></radiobutton>
			<radiobutton name="customResolution" groupName="preferences" text="Use custom scaling"></radiobutton>
		</row>
		<row style="CustomDPIContainer">
			<simplifiednumbertextbox name="scale" width="42"></simplifiednumbertextbox>
			<label>dpi</label>
		</row>
	</preferences>
	<layers title="Layers">
	</layers>
	<measurement title="Measurement">
	</measurement>
	<compareFiles title="Compare Files">
	</compareFiles>
	<modelBrowser title="Model Browser">
	</modelBrowser>
	<displayStyle title=" ">
		<row style="DisplayStyleContentRow">
			<button name="btnWireframe" src="../../../images/Wireframe.svg" tooltip="Armature" style="ViewerButtonCell DisplayStyleButton"></button>
			<button name="btnShaded" src="../../../images/Shaded.svg" tooltip="Grisé" style="ViewerButtonCell DisplayStyleButton"></button>
		</row>
		<row style="DisplayStyleContentRow">
			<button name="btnHiddenLine" src="../../../images/HiddenLine.svg" tooltip="Ligne masquée" style="ViewerButtonCell DisplayStyleButton"></button>
			<button name="btnWireframeOnShaded" src="../../../images/WireframeOnShaded.svg" tooltip="Armature sur grisé" style="ViewerButtonCell DisplayStyleButton"></button>
		</row>
	</displayStyle>
</dialogs>