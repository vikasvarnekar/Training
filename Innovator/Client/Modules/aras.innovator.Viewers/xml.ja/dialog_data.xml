<?xml version="1.0" encoding="UTF-8"?>
<dialogs>
	<find title="検索">
		<row>
			<textbox name="textBoxFind" style="DialogWidget TextBox" width="172"></textbox>
			<button name="buttonNextMatch" src="../../../images/ArrowRight.svg" style="DialogWidget SmallButton"></button>
		</row>
		<row>
			<checkbox name="checkboxMatchCase" label="大文字小文字を区別する" style="DialogWidget"></checkbox>
		</row>
	</find>
	<scribblePalette title="手書き">
		<row>
			<button name="btnScribble" src="../../../images/Scribble.svg" tooltip="手書き" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="選択" style="SelectButton"></button>
		</row>
		<row>
			<button name="smallScribbleButton" src="../../../images/ScribbleSmall.svg" tooltip="小" style="DrawingSizeButton"></button>
			<button name="mediumScribbleButton" src="../../../images/ScribbleMedium.svg" tooltip="中" style="DrawingSizeButton"></button>
			<button name="largeScribbleButton" src="../../../images/ScribbleLarge.svg" tooltip="大" style="DrawingSizeButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Yellow"></color>
					<color color="#008400" title="Green"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="White"></color>
				</row>
				<row>
					<color color="#fd0200" title="Red" selected="selected"></color>
					<color color="#002a99" title="Blue"></color>
					<color color="#808080" title="Grey"></color>
					<color color="#000" title="Black"></color>
				</row>
			</colorpallete>
		</row>
	</scribblePalette>
	<highlightPalette title="ハイライト">
		<row>
			<button name="btnHighlight" src="../../../images/Highlight.svg" tooltip="ボックスをハイライト" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="選択" style="SelectButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Yellow" selected="selected"></color>
					<color color="#4bff4b" title="Green"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="White"></color>
				</row>
				<row>
					<color color="#ff37b6" title="Pink"></color>
					<color color="#01aff1" title="Blue"></color>
					<color color="#808080" title="Grey"></color>
					<color color="#000" title="Black"></color>
				</row>
			</colorpallete>
		</row>
	</highlightPalette>
	<labelPalette title="ラベル">
		<row>
			<button name="btnLabel" src="../../../images/Label.svg" tooltip="ラベル" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="選択" style="SelectButton"></button>
		</row>
		<row>
			<button name="smallLabelButton" src="../../../images/LabelSmall.svg" tooltip="小" style="DrawingSizeButton"></button>
			<button name="mediumLabelButton" src="../../../images/LabelMedium.svg" tooltip="中" style="DrawingSizeButton"></button>
			<button name="largeLabelButton" src="../../../images/LabelLarge.svg" tooltip="大" style="DrawingSizeButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Yellow"></color>
					<color color="#008400" title="Green"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="White"></color>
				</row>
				<row>
					<color color="#fd0200" title="Red" selected="selected"></color>
					<color color="#002a99" title="Blue"></color>
					<color color="#808080" title="Grey"></color>
					<color color="#000" title="Black"></color>
				</row>
			</colorpallete>
		</row>
	</labelPalette>
	<measure title="測定" style="MeasureDialogContainer">
		<row style="MeasureDialogContentRow">
			<button name="btnMeasurePointToPoint" src="../../../images/MeasurePointToPoint.svg" tooltip="点間距離" style="ViewerButtonCell MeasureFirstButton"></button>
			<button name="btnMeasureAngleBetweenFaces" src="../../../images/MeasureAngleBetweenFaces.svg" tooltip="面間角度" style="ViewerButtonCell MeasureSecondButton"></button>
			<button name="btnDeleteAll" src="../../../images/Delete.svg" tooltip="すべて削除" style="ViewerButtonCell MeasureThirdButton"></button>
		</row>
		<row style="MeasureDialogContentRow">
			<button name="btnMeasureEdges" src="../../../images/MeasureEdges.svg" tooltip="エッジ間距離" style="ViewerButtonCell MeasureFourthButton"></button>
			<button name="btnMeasureDistanceBetweenFaces" src="../../../images/MeasureDistanceBetweenFaces.svg" tooltip="面間距離" style="ViewerButtonCell MeasureFifthButton"></button>
			<empty style="ViewerButtonCell ViewerButtonCellEmpty"></empty>
		</row>
	</measure>
	<crossSection title="断面">
		<row style="CrossSectionContentRow">
			<button name="xButton" src="../../../images/XPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="X Plane"></button>
			<button name="yButton" src="../../../images/YPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Y Plane"></button>
			<button name="zButton" src="../../../images/ZPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Z Plane"></button>
			<button name="pButton" src="../../../images/PickPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Pick Plane"></button>

			<button name="tcpvButton" src="../../../images/CuttingPlaneVisibilityToggle.svg" style="DialogWidget PaddedLeftButton SmallButton" tooltip="Toggle Cutting Plane Display"></button>
		</row>
		<row>
			<button name="ixButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Invert Section"></button>
			<button name="iyButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Invert Section"></button>
			<button name="izButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Invert Section"></button>
			<button name="ipButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Invert Section"></button>

			<button name="tcpsButton" src="../../../images/CuttingPlaneSectionToggle.svg" style="DialogWidget PaddedLeftButton SmallButton" tooltip="Toggle Cross Section"></button>
		</row>
		<row style="CappingGeometryRow">
			<checkbox name="checkboxСappingGeometry" label="Capped Geometry" style="DialogWidget"></checkbox>
		</row>

	</crossSection>
	<exploded title="分解立体図">
		<row>
			<slider name="slider" style="DialogWidgetSlider"></slider>
		</row>
	</exploded>
	<comparePallete title="比較">
		<row style="CompareDialogContentRow">
			<button name="btnRollback" src="../../../images/Rollback.svg" tooltip="アラインメントをリセット" style="CompareButton"></button>
			<button name="btnAlignPointToPoint" src="../../../images/AlignPointToPoint.svg" tooltip="ドラッグすると、整列できます。
矢印キーを使用し、微調整を行えます。" style="CompareButton"></button>
			<button name="btnRefresh" src="../../../images/Refresh.svg" tooltip="リフレッシュ" style="CompareButton"></button>
		</row>
		<row style="CompareDialogContentRow">
			<button name="btnDisplayFile1" src="../../../images/File1Indicator.svg" tooltip="ファイル１を表示" style="CompareButton"></button>
			<button name="btnDisplayFile2" src="../../../images/File2Indicator.svg" tooltip="ファイル２を表示" style="CompareButton"></button>
			<button name="btnDisplayBothFiles" src="../../../images/DisplayBothFiles.svg" tooltip="両方のファイルを表示" style="CompareButton"></button>
		</row>
		<row style="CompareDialogContentRow">
			<button name="btnDifferenceView" src="../../../images/ShowComparison.svg" tooltip="差異を表示" style="CompareButton" type="parent">差異</button>
		</row>
		<row style="CompareDialogContentRow CompareToggle">
			<slider name="sliderTolerance" style="DialogWidgetSlider CompareButton"></slider>
		</row>
		<row style="CompareDialogContentRow CompareToggle">
			<colorpallete name="palleteDifferenceColor" style="CompareButton">
				<row>
					<color color="#fdff00" title="Yellow" selected="selected"></color>
					<color color="#4bff4b" title="Green"></color>
					<color color="#fc0001" title="Red"></color>
					<color color="#02249c" title="Blue"></color>
					<color color="#ff37b6" title="Pink"></color>
					<color color="#01aff1" title="Light Blue"></color>
				</row>
			</colorpallete>
		</row>
	</comparePallete>
	<preferences title="Preferences">
		<row>
			<label>Resolution:</label>
		</row>
		<row style="PreferencesRadioButtonContainer">
			<radiobutton name="defaultResolution" groupName="preferences" text="Use system default"></radiobutton>
			<radiobutton name="customResolution" groupName="preferences" text="Use custom scaling"></radiobutton>
		</row>
		<row style="CustomDPIContainer">
			<simplifiednumbertextbox name="scale" width="42"></simplifiednumbertextbox>
			<label>dpi</label>
		</row>
	</preferences>
	<layers title="レイヤー">
	</layers>
	<measurement title="測定">
	</measurement>
	<compareFiles title="ファイル比較">
	</compareFiles>
	<modelBrowser title="モデルブラウザ">
	</modelBrowser>
	<displayStyle title=" "> 
		<row style="DisplayStyleContentRow">
			<button name="btnWireframe" src="../../../images/Wireframe.svg" tooltip="ワイヤーフレーム" style="ViewerButtonCell DisplayStyleButton"></button>
			<button name="btnShaded" src="../../../images/Shaded.svg" tooltip="シェード" style="ViewerButtonCell DisplayStyleButton"></button>
		</row>
		<row style="DisplayStyleContentRow">
			<button name="btnHiddenLine" src="../../../images/HiddenLine.svg" tooltip="線を隠す" style="ViewerButtonCell DisplayStyleButton"></button>
			<button name="btnWireframeOnShaded" src="../../../images/WireframeOnShaded.svg" tooltip="ワイヤーフレーム付きシェード" style="ViewerButtonCell DisplayStyleButton"></button>
		</row>
	</displayStyle>
</dialogs>