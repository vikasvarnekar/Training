<?xml version="1.0" encoding="UTF-8"?>
<dialogs>
	<find title="Suchen">
		<row>
			<textbox name="textBoxFind" style="DialogWidget TextBox" width="172"></textbox>
			<button name="buttonNextMatch" src="../../../images/ArrowRight.svg" style="DialogWidget SmallButton"></button>
		</row>
		<row>
			<checkbox name="checkboxMatchCase" label="Gross-/Kleinschreibung" style="DialogWidget"></checkbox>
		</row>
	</find>
	<scribblePalette title="Skizzieren">
		<row>
			<button name="btnScribble" src="../../../images/Scribble.svg" tooltip="Skizzieren" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="Auswählen" style="SelectButton"></button>
		</row>
		<row>
			<button name="smallScribbleButton" src="../../../images/ScribbleSmall.svg" tooltip="Klein" style="DrawingSizeButton"></button>
			<button name="mediumScribbleButton" src="../../../images/ScribbleMedium.svg" tooltip="Mittel" style="DrawingSizeButton"></button>
			<button name="largeScribbleButton" src="../../../images/ScribbleLarge.svg" tooltip="Gross" style="DrawingSizeButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Gelb"></color>
					<color color="#008400" title="Grün"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="Weiss"></color>
				</row>
				<row>
					<color color="#fd0200" title="Rot" selected="selected"></color>
					<color color="#002a99" title="Blau"></color>
					<color color="#808080" title="Grau"></color>
					<color color="#000" title="Schwarz"></color>
				</row>
			</colorpallete>
		</row>
	</scribblePalette>
	<highlightPalette title="Flächenmarker">
		<row>
			<button name="btnHighlight" src="../../../images/Highlight.svg" tooltip="Flächenmarker" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="Auswählen" style="SelectButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Gelb" selected="selected"></color>
					<color color="#4bff4b" title="Grün"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="Weiss"></color>
				</row>
				<row>
					<color color="#ff37b6" title="Rosa"></color>
					<color color="#01aff1" title="Blau"></color>
					<color color="#808080" title="Grau"></color>
					<color color="#000" title="Schwarz"></color>
				</row>
			</colorpallete>
		</row>
	</highlightPalette>
	<labelPalette title="Textmarker">
		<row>
			<button name="btnLabel" src="../../../images/Label.svg" tooltip="Textmarker" style="DrawingButton"></button>
			<button name="btnSelect" src="../../../images/Select.svg" tooltip="Auswählen" style="SelectButton"></button>
		</row>
		<row>
			<button name="smallLabelButton" src="../../../images/LabelSmall.svg" tooltip="Klein" style="DrawingSizeButton"></button>
			<button name="mediumLabelButton" src="../../../images/LabelMedium.svg" tooltip="Mittel" style="DrawingSizeButton"></button>
			<button name="largeLabelButton" src="../../../images/LabelLarge.svg" tooltip="Gross" style="DrawingSizeButton"></button>
		</row>
		<row>
			<colorpallete name="colorPallete">
				<row>
					<color color="#fdff00" title="Gelb"></color>
					<color color="#008400" title="Grün"></color>
					<color color="#ff7944" title="Orange"></color>
					<color color="#fff" title="Weiss"></color>
				</row>
				<row>
					<color color="#fd0200" title="Rot" selected="selected"></color>
					<color color="#002a99" title="Blau"></color>
					<color color="#808080" title="Grau"></color>
					<color color="#000" title="Schwarz"></color>
				</row>
			</colorpallete>
		</row>
	</labelPalette>
	<measure title="Messen" style="MeasureDialogContainer">
		<row style="MeasureDialogContentRow">
			<button name="btnMeasurePointToPoint" src="../../../images/MeasurePointToPoint.svg" tooltip="Punkt zu Punkt messen" style="ViewerButtonCell MeasureFirstButton"></button>
			<button name="btnMeasureAngleBetweenFaces" src="../../../images/MeasureAngleBetweenFaces.svg" tooltip="Winkel zwischen Flächen messen" style="ViewerButtonCell MeasureSecondButton"></button>
			<button name="btnDeleteAll" src="../../../images/Delete.svg" tooltip="Alles löschen" style="ViewerButtonCell MeasureThirdButton"></button>
		</row>
		<row style="MeasureDialogContentRow">
			<button name="btnMeasureEdges" src="../../../images/MeasureEdges.svg" tooltip="Kanten messen" style="ViewerButtonCell MeasureFourthButton"></button>
			<button name="btnMeasureDistanceBetweenFaces" src="../../../images/MeasureDistanceBetweenFaces.svg" tooltip="Abstand zwischen Flächen messen" style="ViewerButtonCell MeasureFifthButton"></button>
			<empty style="ViewerButtonCell ViewerButtonCellEmpty"></empty>
		</row>
	</measure>
	<crossSection title="Schnitte">
			<row style="CrossSectionContentRow">
				<button name="xButton" src="../../../images/XPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="X Ebene"></button>
				<button name="yButton" src="../../../images/YPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Y Ebene"></button>
				<button name="zButton" src="../../../images/ZPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Z Ebene"></button>
				<button name="pButton" src="../../../images/PickPlane.svg" style="DialogWidget SmallButton BorderedButton" tooltip="Ebene auswählen"></button>

				<button name="tcpvButton" src="../../../images/CuttingPlaneVisibilityToggle.svg" style="DialogWidget PaddedLeftButton SmallButton" tooltip="Schnitt-Ebene Ein-Aus-schalten"></button>
			</row>
			<row>
				<button name="ixButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Sektion invertieren"></button>
				<button name="iyButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Sektion invertieren"></button>
				<button name="izButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Sektion invertieren"></button>
				<button name="ipButton" src="../../../images/InvertSection.svg" style="DialogWidget SmallButton" tooltip="Sektion invertieren"></button>

				<button name="tcpsButton" src="../../../images/CuttingPlaneSectionToggle.svg" style="DialogWidget PaddedLeftButton SmallButton" tooltip="Querschnitte Ein-Aus-schalten"></button>
			</row>
		<row style="CappingGeometryRow">
			<checkbox name="checkboxСappingGeometry" label="begrenzte Geometrie" style="DialogWidget"></checkbox>
		</row>

	</crossSection>
	<exploded title="Explosionsansicht">
		<row>
			<slider name="slider" style="DialogWidgetSlider"></slider>
		</row>
	</exploded>
	<comparePallete title="Vergleichen">
		<row style="CompareDialogContentRow">
			<button name="btnRollback" src="../../../images/Rollback.svg" tooltip="Ausrichtung zurücksetzen" style="CompareButton"></button>
			<button name="btnAlignPointToPoint" src="../../../images/AlignPointToPoint.svg" tooltip="Punkt zu Punkt ausrichten (ziehen)
Pfeiltasten für Fein-Ausrichtung" style="CompareButton"></button>
			<button name="btnRefresh" src="../../../images/Refresh.svg" tooltip="neu Laden" style="CompareButton"></button>
		</row>
		<row style="CompareDialogContentRow">
			<button name="btnDisplayFile1" src="../../../images/File1Indicator.svg" tooltip="Datei 1 anzeigen" style="CompareButton"></button>
			<button name="btnDisplayFile2" src="../../../images/File2Indicator.svg" tooltip="Datei 2 anzeigen" style="CompareButton"></button>
			<button name="btnDisplayBothFiles" src="../../../images/DisplayBothFiles.svg" tooltip="Beide Dateien anzeigen" style="CompareButton"></button>
		</row>
		<row style="CompareDialogContentRow">
			<button name="btnDifferenceView" src="../../../images/ShowComparison.svg" tooltip="Unterschiede anzeigen" style="CompareButton" type="parent">Difference</button>
		</row>
		<row style="CompareDialogContentRow CompareToggle">
			<slider name="sliderTolerance" style="DialogWidgetSlider CompareButton"></slider>
		</row>
		<row style="CompareDialogContentRow CompareToggle">
			<colorpallete name="palleteDifferenceColor" style="CompareButton">
				<row>
					<color color="#fdff00" title="Gelb" selected="selected"></color>
					<color color="#4bff4b" title="Grün"></color>
					<color color="#fc0001" title="Rot"></color>
					<color color="#02249c" title="Blau"></color>
					<color color="#ff37b6" title="Rosa"></color>
					<color color="#01aff1" title="Hellblau"></color>
				</row>
			</colorpallete>
		</row>
	</comparePallete>
	<preferences title="Präferenzen">
		<row>
			<label>Auflösung:</label>
		</row>
		<row style="PreferencesRadioButtonContainer">
			<radiobutton name="defaultResolution" groupName="preferences" text="Systemstandard nutzen"></radiobutton>
			<radiobutton name="customResolution" groupName="preferences" text="Eigene Skalierung nutzen"></radiobutton>
		</row>
		<row style="CustomDPIContainer">
			<simplifiednumbertextbox name="scale" width="42"></simplifiednumbertextbox>
			<label>dpi</label>
		</row>
	</preferences>
	<layers title="Lagen">
	</layers>
	<measurement title="Messen">
	</measurement>
	<compareFiles title="Dateien Vergleichen">
	</compareFiles>
	<modelBrowser title="Model Browser">
	</modelBrowser>
	<displayStyle title=" ">
		<row style="DisplayStyleContentRow">
			<button name="btnWireframe" src="../../../images/Wireframe.svg" tooltip="Drahtmodell" style="ViewerButtonCell DisplayStyleButton"></button>
			<button name="btnShaded" src="../../../images/Shaded.svg" tooltip="Schattiert" style="ViewerButtonCell DisplayStyleButton"></button>
		</row>
		<row style="DisplayStyleContentRow">
			<button name="btnHiddenLine" src="../../../images/HiddenLine.svg" tooltip="Verdeckte Linien" style="ViewerButtonCell DisplayStyleButton"></button>
			<button name="btnWireframeOnShaded" src="../../../images/WireframeOnShaded.svg" tooltip="Drahtmodell und schattiert" style="ViewerButtonCell DisplayStyleButton"></button>
		</row>
	</displayStyle>
</dialogs>