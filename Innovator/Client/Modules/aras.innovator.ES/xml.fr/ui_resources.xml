	<!--Syntax Help-->
	<resource key="syntax.title" value="Syntaxe Solr est utilisée pour les critères de recherche suivants:" />
	<resource key="syntax.and_description" value="&lt;span&gt;test AND roam&lt;/span&gt; ou &lt;span&gt;test &amp;&amp; roam&lt;/span&gt; correspondra aux résultats contenant «test» et «roam» tous les deux" />
	<resource key="syntax.or_description" value="&lt;span&gt;test OR roam&lt;/span&gt; ou &lt;span&gt;test || roam&lt;/span&gt; correspondra aux résultats contenant «test» ou «roam» ou encore tous les deux " />
	<resource key="syntax.not_description" value="&lt;span&gt;test NOT roam&lt;/span&gt; ou &lt;span&gt;test ! roam&lt;/span&gt; correspondra aux résultats contenant «test» sans «roam» " />
	<resource key="syntax.not_sub_description" value="NOT ou !  affecte le mot qui suit l’opérateur " />
	<resource key="syntax.plus_description" value="&lt;span&gt;+test roam&lt;/span&gt; correspondra aux résultats qui doivent contenir «test» et peuvent contenir ou ne pas contenir «roam» " />
	<resource key="syntax.plus_sub_description" value="Utilisable à l’avant du mot uniquement" />
	<resource key="syntax.minus_description" value="&lt;span&gt;-test&lt;/span&gt; correspondra aux résultats qui ne contiennent pas de «test»" />
	<resource key="syntax.minus_sub_description" value="Utilisable à l’avant du mot uniquement" />
	<resource key="syntax.parenthesis_description" value="Les parenthèses peuvent être utilisées pour regrouper les termes.  C’est un usage commun pour la logique de contrôle booléen." />
	<resource key="syntax.multiply_description" value="&lt;span&gt;tes*&lt;/span&gt; correspondra au «test», «testing» et «tester» " />
	<resource key="syntax.question_description" value="&lt;span&gt;te?t&lt;/span&gt; correspondra à tous les deux «test» et «text». Utilisez ? seulement pour les autres lettres" />
	<resource key="syntax.tilda_description" value="&lt;span&gt;roam~&lt;/span&gt; correspondra à «roam», «roams», «foam» et «foams»" />
	<resource key="syntax.tilda_sub_description" value="Utilisable à la fin du mot uniquement " />
	<resource key="syntax.proximity_description" value="&lt;span&gt;“test roam”~5&lt;/span&gt; correspondra aux résultats qui utilisent «test» et «roam» à la portée de cinq mots de l’un à l’autre" />
	<resource key="syntax.proximity_sub_description" value="Utilisable à la fin du mot uniquement" />
	<resource key="syntax.boosting_description" value="L’ajout du symbole caret à la fin du mot, par exemple, &lt;span&gt;test^4&lt;/span&gt; augmentera l’importance du mot et les objets avec ce mot apparaîtront plus haut dans la recherche" />
	<resource key="syntax.boosting_sub_description" value="Utilisable à la fin du mot uniquement" />
	<resource key="syntax.phrase_description" value="&lt;span&gt;“test roam”&lt;/span&gt; correspondra aux résultats contenant la phrase entière : «test roam»" />

