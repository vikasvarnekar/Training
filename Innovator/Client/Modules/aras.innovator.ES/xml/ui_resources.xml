	<!--Syntax Help-->
	<resource key="syntax.title" value="Solr syntax is used for search criteria:" />
	<resource key="syntax.and_description" value="&lt;span&gt;test AND roam&lt;/span&gt; or &lt;span&gt;test &amp;&amp; roam&lt;/span&gt; will match results that contain both test and roam" />
	<resource key="syntax.or_description" value="&lt;span&gt;test OR roam&lt;/span&gt; or &lt;span&gt;test || roam&lt;/span&gt; will match results that contain either test or roam or both" />
	<resource key="syntax.not_description" value="&lt;span&gt;test NOT roam&lt;/span&gt; or &lt;span&gt;test ! roam&lt;/span&gt; will match results that contain test but not roam" />
	<resource key="syntax.not_sub_description" value="NOT or ! acts on the word following the operator" />
	<resource key="syntax.plus_description" value="&lt;span&gt;+test roam&lt;/span&gt; will match results that must contain test and may or may not contain roam" />
	<resource key="syntax.plus_sub_description" value="use at beginning of word only" />
	<resource key="syntax.minus_description" value="&lt;span&gt;-test&lt;/span&gt; will match results that do not contain test" />
	<resource key="syntax.minus_sub_description" value="use at beginning of word only" />
	<resource key="syntax.parenthesis_description" value="Parenthesis may be used to group terms. This is commonly useful for controlling boolean logic" />
	<resource key="syntax.multiply_description" value="&lt;span&gt;tes*&lt;/span&gt; will match test, testing and tester" />
	<resource key="syntax.question_description" value="&lt;span&gt;te?t&lt;/span&gt; will match both test and text. Use ? only with another letters" />
	<resource key="syntax.tilda_description" value="&lt;span&gt;roam~&lt;/span&gt; will match roam, roams, foam and foams" />
	<resource key="syntax.tilda_sub_description" value="use at end of word only" />
	<resource key="syntax.proximity_description" value="&lt;span&gt;“test roam”~5&lt;/span&gt; will match results that use test and roam within 5 words of each other" />
	<resource key="syntax.proximity_sub_description" value="use at end of word only" />
	<resource key="syntax.boosting_description" value="adding the carat symbol to the end of a term, such as &lt;span&gt;test^4&lt;/span&gt; will increase the relevance of the term and items with that term may appear higher in the results" />
	<resource key="syntax.boosting_sub_description" value="use at end of word only" />
	<resource key="syntax.phrase_description" value="&lt;span&gt;“test roam”&lt;/span&gt; will match results containing the full phrase: test roam" />

