	<!--Syntax Help-->
	<resource key="syntax.title" value="Solr-Syntax wird für Suchkriterien verwendet:" />
	<resource key="syntax.and_description" value="&lt;span&gt;test AND roam&lt;/span&gt; oder &lt;span&gt;test &amp;&amp; roam&lt;/span&gt; werden den Ergebnissen entsprechen, die beides ‘test’ und ‘roam’ enthalten" />
	<resource key="syntax.or_description" value="&lt;span&gt;test OR roam&lt;/span&gt; oder &lt;span&gt;test || roam&lt;/span&gt; werden den Ergebnissen entsprechen, die weder ‘test’ noch ‘roam’ noch beides enthalten" />
	<resource key="syntax.not_description" value="&lt;span&gt;test NOT roam&lt;/span&gt; oder &lt;span&gt;test ! roam&lt;/span&gt; werden den Ergebnissen entsprechen, die ‘test’, aber kein ‘roam’ enthalten" />
	<resource key="syntax.not_sub_description" value="NOT or ! wirkt auf das nach dem Operator folgende Wort ein" />
	<resource key="syntax.plus_description" value="&lt;span&gt;+test roam&lt;/span&gt; wird den Ergebnissen entsprechen, die ‘test’ enthalten müssen und ‘roam’ enthalten können bzw. nicht enthalten" />
	<resource key="syntax.plus_sub_description" value="Wird nur am Anfang des Wortes verwendet" />
	<resource key="syntax.minus_description" value="&lt;span&gt;-test&lt;/span&gt; wird den Ergebnissen entsprechen, die keinen ‘test’ enthalten" />
	<resource key="syntax.minus_sub_description" value="Wird nur am Anfang des Wortes verwendet" />
	<resource key="syntax.parenthesis_description" value="Runde Klammern können für Gruppierungsbedingungen verwendet werden. Es ist im Allgemeinen hilfreich, um Boolean-Logik zu kontrollieren." />
	<resource key="syntax.multiply_description" value="&lt;span&gt;tes*&lt;/span&gt; wird dem ‘test’, ‘testing’ und ‘tester’ entsprechen" />
	<resource key="syntax.question_description" value="&lt;span&gt;te?t&lt;/span&gt; wird sowie dem Wort ‘test’, als auch ‘text’ entsprechen. Verwenden Sie ? nur mit anderen Buchstaben" />
	<resource key="syntax.tilda_description" value="&lt;span&gt;roam~&lt;/span&gt; wird ‘roam’, ‘roams’, ‘foam’ und ‘foams’ entsprechen" />
	<resource key="syntax.tilda_sub_description" value="Wird nur am Ende des Wortes verwendet" />
	<resource key="syntax.proximity_description" value="&lt;span&gt;“test roam”~5&lt;/span&gt; wird den Ergebnissen entsprechen, die ‘test’ und ‘roam’ innerhalb von 5 Wörter voneinander verwenden." />
	<resource key="syntax.proximity_sub_description" value="Wird nur am Ende des Wortes verwendet" />
	<resource key="syntax.boosting_description" value="Hinzufügen des Caretsymbols am Ende der Bedingung – so wie &lt;span&gt;test^4&lt;/span&gt; – wird die Relevanz der Bedingung erhöhen und die Objekte mit dieser Bedingung können höher in den Ergebnissen erscheinen." />
	<resource key="syntax.boosting_sub_description" value="Wird nur am Ende des Wortes verwendet" />
	<resource key="syntax.phrase_description" value="&lt;span&gt;“test roam”&lt;/span&gt; wird den Ergebnissen entsprechen, die die ganze Phrase enthalten: ‘test roam’" />

