<?xml version="1.0" encoding="utf-8" ?>
<toolbarapplet on_load="onToolbarAppletLoad" on_click="onButtonClick" buttonstyle="windows" buttonsize="26,25">
	<toolbar id="viewerbar">
		<button image="../images/Save.svg" id="save_report" tooltip="レポートを保存する" disabled="true"/>
		<separator/>
		<button image="../images/Print.svg" id="print_report" tooltip="印刷" disabled="true"/>
		<separator/>
		<button image="../images/PDF.svg" id="export_pdf" tooltip="PDFに出力" disabled="true"/>
		<button image="../images/ExportToExcel.svg" id="export_excel" tooltip="Excelとしてエクスポートする" disabled="true"/>
		<button image="../images/ExportToWord.svg" id="export_word" tooltip="Wordとしてエクスポートする" disabled="true"/>
		<button image="../images/CSV.svg" id="export_csv" tooltip="CSVに出力" disabled="true"/>
		<button image="../images/XML.svg" id="export_xml" tooltip="Xmlに出力" disabled="true"/>
		<button image="../images/SQL.svg" id="export_sql" tooltip="SQLに出力" disabled="true"/>
		<separator/>
		<choice width="auto" id="results" tooltip="検索結果の表示件数" text="結果" disabled="true">
			<choiceitem id="1">1 件</choiceitem>
			<choiceitem id="10">10 件</choiceitem>
			<choiceitem id="100">100 件</choiceitem>
			<choiceitem id="1000">1000 件</choiceitem>
			<choiceitem id="10000">10000 件</choiceitem>
			<choiceitem id="-1">全件</choiceitem>
		</choice>
		<separator/>
		<button image="../Modules/aras.innovator.Izenda/images/UnlockAndClose.svg" id="unlock_close" tooltip="アンロック &amp;閉じる" disabled="true"/>
		<separator/>
		<button image="../images/Help.svg" id="help" tooltip="ヘルプ"/>
		<separator/>
		<button image="../images/SaveUnlockClose.svg" id="save_unlock_close" tooltip="保存、アンロック&amp;閉じる" disabled="true"/>
	</toolbar>
</toolbarapplet>