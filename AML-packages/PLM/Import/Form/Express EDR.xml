      <html_code><![CDATA[<span class="sys_f_label_container sys_item_link" onclick="linkClick(); return false;">
	<b>
		Link for ECO
	</b>
</span>
<script type="text/javascript">
function getRelateEcoName() {
	var aml = '<AML><Item type="Express ECO EDR" action="get" select="source_id">'
	+ '<related_id>'
	+ '<Item type="Express EDR">'
	+ '<id>' + document.thisItem.getID() + '</id>'
	+ '</Item>'
	+ '</related_id>'
	+ '</Item></AML>';
	var queryItem = aras.newIOMItem("Express ECO EDR", "");
	queryItem.loadAML(aml);
	queryItem = queryItem.apply();
	if (queryItem.isError()) {
		throw new InvalidOperationException("Failed to get Express ECO EDR. Details:" + ecoEdrItem);
	}
	getRelateEcoName.ecoId = queryItem.getProperty("source_id");
	var ecoQuery = aras.newIOMItem("Express ECO", "get");
	ecoQuery.setAttribute("select", "item_number");
	ecoQuery.setAttribute("id", getRelateEcoName.ecoId);
	ecoQuery = ecoQuery.apply();
	if (ecoQuery.isError()) {
		throw new InvalidOperationException("Failed to get Express ECO. Details:" + ecoEdrItem);
	}
	var ecoNumber = ecoQuery.getProperty("item_number");
	var spanElement = document.querySelector("[id=\"" + document.fieldsTab["related_eco"] + "\"] span");
	spanElement.textContent = ecoNumber;
}
function linkClick() {
	var ecoItem = aras.getItemById("Express ECO", getRelateEcoName.ecoId);
	if (ecoItem) aras.uiShowItemEx(ecoItem, undefined);
};
getRelateEcoName();
</script>


]]></html_code>
