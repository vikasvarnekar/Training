    <css>.contentFieldContainer {
	width: 777px;
	height: 480px;
	border:1px solid #C0C0C0;
}

.aceEditorContainer {
	width: 100%;
	height: 100%;
}

.aceEditorContainer.readOnly .ace_scroller {
	background-color: #FAFAFA;
}

.ace_changed-line {
	position: absolute;
	width: 5px;
	background-color: #E78E46;
}

.validationMessage {
	position: absolute;
	z-index: 10;
	opacity: 0;
	right: 30px;
	top: 25px;
	height: 32px;
	padding: 0 10px 0px 38px;
	border: 1px solid #C0C0C0;
	background-color: #FFFFFF;
	background-size: 28px 28px;
	background-position: 5px 2px;
	background-repeat: no-repeat;
	background-image: url('../../images/SaveUnlockClose.svg?svm=1');
	line-height: 32px;
	font-size: 12px;
}

.validationMessage.invalid{
	background-image: url('../../images/Error.svg?svm=1');
}

.validationMessage.active {
	animation: fadeValidationMessage  1.5s;
}

@keyframes fadeValidationMessage {
	0% {opacity: 1;}
	70% {opacity: 1;}
	100% {opacity: 0;}
}

.ace_error.ace_gutter-cell {
	color: #BF2E0B;
	font-weight: bold;
}
</css>
