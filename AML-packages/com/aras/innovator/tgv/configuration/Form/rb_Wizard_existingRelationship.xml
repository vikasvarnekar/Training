      <html_code><![CDATA[<script>
 this.onload = function() {
  var contextItemTypeId = document.thisItem.getProperty('item_type');
  var relationshipNameComponent = this['field97B75818E9D349998726CACA1398858CComponent'];

  var node = aras.getItemTypeForClient(contextItemTypeId, 'id').node;
  var relationships = node.selectNodes('Relationships/Item[@type=\'RelationshipType\']');
  var arr = [];

  for (var i = 0; i < relationships.length; i++) {
   arr.push({
    label: aras.getItemProperty(relationships[i], 'label') || aras.getItemProperty(relationships[i], 'name'),
    value: aras.getItemProperty(relationships[i], 'id')
   });
  }

  if (arr.length) {
   relationshipNameComponent.component.state.list = arr;
  }
 };
</script>]]></html_code>
