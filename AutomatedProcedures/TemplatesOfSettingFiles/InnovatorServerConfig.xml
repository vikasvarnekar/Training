<?xml version="1.0" encoding="UTF-8"?>
<Innovator>
	<UI-Tailoring product_name="Aras Innovator"/>
	<operating_parameter key="debug_log_flag" value="false"/>
	<operating_parameter key="debug_log_limit" value="10000"/>
	<operating_parameter key="debug_log_pretty" value="true"/>
	<disabled_operating_parameter key="performance_logging" value="cpu"/>
	<operating_parameter key="xslt_processor_debug" value="false"/>
	<disabled_operating_parameter key="email_debug_option" value="file"/>
	<License lic_type="[Innovator.License.Type]" lic_key="[Innovator.License.Key]" act_key="[Innovator.Activation.Key]" />
	<Mail SMTPServer="[SMTP.Server]"/>
	<operating_parameter key="temp_folder" value=".\Innovator\Server\temp\"/>
	<operating_parameter key="ServerMethodTempDir" value=".\Innovator\Server\dll\"/>
	<operating_parameter key="debug_log_prefix" value=".\Innovator\Server\logs\"/>
	<AgentService InnovatorToServiceAddress="[AgentServiceBindingEndpoint]"/>
	<OAuthServerDiscovery>
		<Urls>
			<Url value="$[HTTP_PREFIX_SERVER]$[HTTP_HOST_SERVER]$[HTTP_PORT_SERVER]$[HTTP_PATH_SERVER]/OAuthServer/"/>
		</Urls>
	</OAuthServerDiscovery>
</Innovator>
