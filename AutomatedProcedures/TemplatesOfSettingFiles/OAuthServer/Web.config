		<handlers>
			<add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
		</handlers>
		<aspNetCore processPath="dotnet" arguments=".\Bin\Aras.OAuth.Server.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout">
